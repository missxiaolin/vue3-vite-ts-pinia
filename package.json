{
	"name": "web",
	"version": "1.0.0",
	"scripts": {
		"serve": "vite --force",
		"build": "npm run lint:prettier && vite build",
		"preview": "vite preview",
		"lint:eslint": "eslint --cache --max-warnings 0  \"src/**/*.{vue,ts,js,tsx}\" --fix",
		"lint:prettier": "prettier --write  \"src/**/*.{ts,js,json,tsx,css,less,scss,vue,html,md}\"",
		"lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
		"lint:pretty": "pretty-quick --staged",
		"prepare": "husky install",
		"commit": "lint-staged"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run commit",
			"commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
		}
	},
	"lint-staged": {
		"*.md": "prettier --write",
		"*.{ts,tsx,js,vue,scss}": "prettier --write"
	},
	"dependencies": {
		"ant-design-vue": "4.2.3",
		"autoprefixer": "^10.4.20",
		"axios": "^0.24.0",
		"babel-polyfill": "^6.26.0",
		"clipboard": "^2.0.10",
		"echarts": "^5.4.2",
		"lodash": "^4.17.21",
		"mitt": "^3.0.0",
		"moment": "^2.29.1",
		"numbro": "^2.3.6",
		"pinia": "^2.0.9",
		"pinia-plugin-persist": "0.0.92",
		"react-dnd-html5-backend": "^16.0.1",
		"sortablejs": "^1.15.0",
		"tailwindcss": "2.2.19",
		"vite": "^3.0.4",
		"vue": "^3.2.30",
		"vue-click-outside": "^1.1.0",
		"vue-router": "^4.0.12",
		"vue3-dnd": "^2.0.1",
		"xlsx": "^0.18.5"
	},
	"devDependencies": {
		"@commitlint/cli": "^10.0.0",
		"@commitlint/config-conventional": "^10.0.0",
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "^5.9.1",
		"@typescript-eslint/parser": "^5.3.0",
		"@vitejs/plugin-basic-ssl": "^0.1.1",
		"@vitejs/plugin-legacy": "^2.0.0",
		"@vitejs/plugin-vue": "^1.9.3",
		"@vitejs/plugin-vue-jsx": "^1.2.0",
		"@vue/compiler-sfc": "^3.2.30",
		"@vue/eslint-config-prettier": "^6.0.0",
		"@vue/eslint-config-typescript": "^5.0.2",
		"@vueuse/core": "^7.1.2",
		"crypto-js": "^4.1.1",
		"eslint": "^7.2.0",
		"eslint-config-airbnb-base": "^14.2.1",
		"eslint-define-config": "^1.2.4",
		"eslint-plugin-import": "^2.25.2",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-vue": "^8.0.3",
		"husky": "^7.0.4",
		"lint-staged": "^12.3.5",
		"postcss": "^8.4.41",
		"postcss-html": "^1.2.0",
		"postcss-sass": "^0.5.0",
		"prettier": "^2.4.1",
		"pretty-quick": "^3.1.3",
		"regenerator-runtime": "^0.13.9",
		"rollup-plugin-commonjs": "^10.1.0",
		"rollup-plugin-external-globals": "^0.6.1",
		"rollup-plugin-visualizer": "^5.5.2",
		"sass": "^1.43.4",
		"stylelint": "^14.0.1",
		"stylelint-config-standard": "^23.0.0",
		"stylelint-scss": "^4.0.0",
		"terser": "^5.14.2",
		"typescript": "^4.4.4",
		"unplugin-auto-import": "^0.5.11",
		"unplugin-vue-components": "^0.17.13",
		"vite-plugin-cdn-import": "^0.3.5",
		"vite-plugin-components": "^0.13.3",
		"vite-plugin-compression": "^0.3.5",
		"vite-plugin-html": "^2.1.2",
		"vite-plugin-vue-setup-extend": "^0.4.0",
		"vue-eslint-parser": "^8.0.1",
		"vue-global-api": "^0.4.1",
		"vue-tsc": "^0.3.0"
	},
	"__npminstall_done": false
}
